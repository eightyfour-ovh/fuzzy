parameters:
  tmpDir: var/cache/phpstan
  bootstrapFiles:
    - vendor/autoload.php
    - tests/bootstrap.php
  scanDirectories:
    - vendor
  level: 9
  paths:
    - src
    - tests
  checkGenericClassInNonGenericObjectType: false
  ignoreErrors:
    - '#(.*)no value type specified in iterable type array.#'
